<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.test">

    <application
        android:name="debug.TestApplication"
        android:allowBackup="false"
        android:icon="@mipmap/test_ic_launcher"
        android:label="@string/test_app_name"
        android:resizeableActivity="true"
        android:roundIcon="@mipmap/test_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/test_AppTheme"
        tools:replace="android:label">
        <activity android:name=".TestActivity" />
        <activity android:name="debug.activity.TestMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>